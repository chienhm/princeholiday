        <servlet-name>GetFollow</servlet-name>
        <url-pattern>/getfollow</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetPrice</servlet-name>
        <servlet-class>com.morntea.web.pricemonitor.servlet.GetPriceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetPrice</servlet-name>
        <url-pattern>/getprice</url-pattern>
    </servlet-mapping>
    
    
	<servlet>
		<servlet-name>AddItem</servlet-name>
		<servlet-class>com.morntea.web.pricemonitor.servlet.AddItemServlet</servlet-class>
	</servlet>
