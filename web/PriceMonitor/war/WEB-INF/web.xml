        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
	<servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>com.morntea.web.pricemonitor.servlet.RegisterServlet</servlet-class>
