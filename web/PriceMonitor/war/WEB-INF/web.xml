	<servlet>
		<servlet-name>Test</servlet-name>
		<servlet-class>com.morntea.web.pricemonitor.servlet.TestServlet</servlet-class>
	</servlet>
	
    <servlet-mapping>
        <servlet-name>soundbook</servlet-name>
        <url-pattern>/soundbook</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>soundbook</servlet-name>
        <servlet-class>com.morntea.web.guestbook.SoundBookGuestbookServlet</servlet-class>
    </servlet>
    
	<!-- Family Servlets -->
    <servlet-mapping>
        <servlet-name>member</servlet-name>
        <url-pattern>/member</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>member</servlet-name>
        <servlet-class>com.morntea.web.family.MemberServlet</servlet-class>
    </servlet>
     
	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
