		<servlet-name>AddItem</servlet-name>
		<url-pattern>/additem</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ProductMonitor</servlet-name>
		<servlet-class>com.morntea.web.pricemonitor.servlet.ProductMonitorServlet</servlet-class>
	</servlet>
